# Sample LDIF data otherobjects ldap integration
#
# Some users
# userpassword for all persons is "pass"
#
version: 1

dn: ou=users,dc=otherobjects,dc=org
objectclass: organizationalUnit
objectclass: top
description: Contains otherobjects users
ou: users

dn: ou=groups,dc=otherobjects,dc=org
objectclass: organizationalUnit
objectclass: top
description: Contains entries which describe groups 
ou: groups


# Admin
# --------------

dn: uid=admin,ou=users,dc=otherobjects,dc=org
objectclass: person
objectclass: organizationalPerson
objectclass: inetOrgPerson
objectclass: top
cn: Administrator
description: The Administrator
givenname: admin
sn: admin
uid: admin
mail: admin@otherobjects.org
userpassword: {SHA}nU4eI71bcnBGqeO0t9tXvY1u5oQ=



dn: cn=Administrators,ou=groups,dc=otherobjects,dc=org
objectclass: groupOfUniqueNames
objectclass: top
cn: Administrators
uniquemember: uid=admin,ou=users,dc=otherobjects,dc=org

# Common users
# --------------

dn: uid=user1,ou=users,dc=otherobjects,dc=org
objectclass: person
objectclass: organizationalPerson
objectclass: inetOrgPerson
objectclass: top
cn: User
description: A User
givenname: user1
sn: user
uid: user1
mail: user1@otherobjects.org
userpassword: {SHA}nU4eI71bcnBGqeO0t9tXvY1u5oQ=

dn: uid=user2,ou=users,dc=otherobjects,dc=org
objectclass: person
objectclass: organizationalPerson
objectclass: inetOrgPerson
objectclass: top
cn: User
description: A User
givenname: user2
sn: user
uid: user2
mail: user2@otherobjects.org
userpassword: {SHA}nU4eI71bcnBGqeO0t9tXvY1u5oQ=



dn: cn=Users,ou=groups,dc=otherobjects,dc=org
objectclass: groupOfUniqueNames
objectclass: top
cn: Users
uniquemember: uid=user1,ou=users,dc=otherobjects,dc=org
uniquemember: uid=user2,ou=users,dc=otherobjects,dc=org





