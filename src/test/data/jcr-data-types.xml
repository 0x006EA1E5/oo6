<?xml version="1.0" encoding="UTF-8"?>
<types xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:rep="internal" xmlns:oo="http://www.otherobjects.org/system/oo"
	xmlns:ocm="http://jackrabbit.apache.org/ocm" xmlns:sv="http://www.jcp.org/jcr/sv/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" jcr:primaryType="oo:node" label="Data Type" cssClass="datatypes">

	<!-- Site -->
	<org.otherobjects.cms.model.SiteFolder jcr:primaryType="oo:node" jcr:uuid="4b299ada-1f0e-4999-a623-e1ddb3eaab7e" labelProperty="label" ocm:classname="org.otherobjects.cms.types.TypeDef"
		className="org.otherobjects.cms.model.SiteFolder">
		<properties jcr:primaryType="nt:unstructured">
			<property jcr:primaryType="nt:unstructured" required="true" type="string" ocm:classname="org.otherobjects.cms.types.PropertyDef" name="code" />
			<property jcr:primaryType="nt:unstructured" type="string" ocm:classname="org.otherobjects.cms.types.PropertyDef" name="label" />
			<property jcr:primaryType="nt:unstructured" type="string" ocm:classname="org.otherobjects.cms.types.PropertyDef" name="cssClass" />
			<property jcr:primaryType="nt:unstructured" type="list" label="${allowedTypes.label}" collectionElementType="reference" relatedType="org.otherobjects.cms.types.TypeDef"
				ocm:classname="org.otherobjects.cms.types.PropertyDef" name="allowedTypes" />
		</properties>
	</org.otherobjects.cms.model.SiteFolder>
	<Article jcr:primaryType="oo:node" ocm:classname="org.otherobjects.cms.types.TypeDef" labelProperty="title">
		<properties>
			<property1 ocm:classname="org.otherobjects.cms.types.PropertyDef" name="title" type="string" required="true" />
			<property2 ocm:classname="org.otherobjects.cms.types.PropertyDef" name="teaser" type="text" />
			<property3 ocm:classname="org.otherobjects.cms.types.PropertyDef" name="content" type="html" />
			<property4 ocm:classname="org.otherobjects.cms.types.PropertyDef" name="tags" type="text" />
		</properties>
	</Article>

	<org.otherobjects.cms.model.CmsImage jcr:primaryType="oo:node" label="Image" jcr:uuid="8b79bc82-d4a6-4a22-aaff-8c216f5bc5d6" labelProperty="label"
		ocm:classname="org.otherobjects.cms.types.TypeDef" className="org.otherobjects.cms.model.CmsImage">
		<properties jcr:primaryType="nt:unstructured">
			<property jcr:primaryType="nt:unstructured" type="string" ocm:classname="org.otherobjects.cms.types.PropertyDef" name="code" />
			<property jcr:primaryType="nt:unstructured" type="string" ocm:classname="org.otherobjects.cms.types.PropertyDef" name="label" />
			<property jcr:primaryType="nt:unstructured" type="string" ocm:classname="org.otherobjects.cms.types.PropertyDef" name="description" />
			<property jcr:primaryType="nt:unstructured" type="string" ocm:classname="org.otherobjects.cms.types.PropertyDef" name="keywords" />
			<property jcr:primaryType="nt:unstructured" type="string" ocm:classname="org.otherobjects.cms.types.PropertyDef" name="copyright" />
			<property jcr:primaryType="nt:unstructured" type="string" ocm:classname="org.otherobjects.cms.types.PropertyDef" name="originalFileId" required="true" />
			<property jcr:primaryType="nt:unstructured" type="number" ocm:classname="org.otherobjects.cms.types.PropertyDef" name="originalWidth" required="true" />
			<property jcr:primaryType="nt:unstructured" type="number" ocm:classname="org.otherobjects.cms.types.PropertyDef" name="originalHeight" required="true" />
			<property jcr:primaryType="nt:unstructured" type="string" ocm:classname="org.otherobjects.cms.types.PropertyDef" name="originalProvider" />
			<property jcr:primaryType="nt:unstructured" type="string" ocm:classname="org.otherobjects.cms.types.PropertyDef" name="originalId" />
		</properties>
	</org.otherobjects.cms.model.CmsImage>

</types>
