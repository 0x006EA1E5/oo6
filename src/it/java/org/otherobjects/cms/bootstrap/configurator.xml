<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd 
		http://www.springframework.org/schema/context 
		http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<!-- For properties -->
	<bean id="otherObjectsConfigurator" class="org.otherobjects.cms.test.TestOtherObjectsConfigurator">
		<property name="ignoreResourceNotFound" value="true" />
		<property name="locations">
			<list>
				<value>classpath:otherobjects.resources/config/otherobjects.properties</value>
			</list>
		</property>
	</bean>
	
	<bean id="resourceCleaner" class="org.otherobjects.cms.test.ResourceCleaner" init-method="cleanup">
		<property name="resource" value="file:./target/data"></property>
	</bean>
	
	
	

</beans>