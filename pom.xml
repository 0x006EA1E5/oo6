	<!-- ** Properties ********************************************************************************************* -->
	<!-- *********************************************************************************************************** -->
	<properties>
		<data.path>${project.build.directory}/data/</data.path>
		<jdbc.groupId>com.h2database</jdbc.groupId>
		<jdbc.artifactId>h2</jdbc.artifactId>
		<jdbc.version>1.0.79</jdbc.version>
		<jdbc.driverClassName>org.h2.Driver</jdbc.driverClassName>
		<jdbc.url>jdbc:h2:${data.path}database</jdbc.url>
		<jdbc.username>admin</jdbc.username>
		<jdbc.password>admin</jdbc.password>
		<jcr.config.path>classpath:otherobjects.resources/config/jackrabbit-repo.xml</jcr.config.path>
		<test.jcr.config.path>classpath:otherobjects.resources/config/jackrabbit-repo.xml</test.jcr.config.path>
		<jcr.repository.location>${data.path}repository/</jcr.repository.location>
		<jcr.username>admin</jcr.username>
		<jcr.password>admin</jcr.password>
		<!-- Shared version number properties -->
		<org.springframework.version>3.0.5.RELEASE</org.springframework.version>
		<org.apache.jackrabbit.version>2.2.5</org.apache.jackrabbit.version>
