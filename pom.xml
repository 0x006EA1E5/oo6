				<version>1.0-beta-2</version>
			</extension>
		</extensions>
		<plugins>
			<!--
				Documentation generation <plugin> <groupId>org.jboss.maven.plugins</groupId> <artifactId>maven-jdocbook-plugin</artifactId> <extensions>true</extensions> <version>2.1.1</version> <configuration>
				<formats> <format> <formatName>pdf</formatName> <stylesheetResource>file:${basedir}/src/main/docbook/xslt/pdf/main-pdf.xsl</stylesheetResource> </format> <format> <formatName>html</formatName>
				</format> </formats> <sourceDocumentName>${docbook.name}.xml</sourceDocumentName> <imageResource> <directory>src/main/docbook/images</directory> <targetPath>images</targetPath>
				<filtering>false</filtering> </imageResource> <options> <useRelativeImageUris>false</useRelativeImageUris> <xincludeSupported>false</xincludeSupported> </options> </configuration> </plugin>
			-->
			<plugin>
				<artifactId>maven-antrun-plugin</artifactId>
				<executions>
					<execution>
						<phase>validate</phase>
						<goals>
							<goal>run</goal>
						</goals>
						<configuration>
							<tasks>
								<echo>********** Cleaning up source files **************</echo>
								<fixcrlf srcdir="src" eol="unix" eof="remove" tab="remove" tablength="4" fixlast="true" encoding="UTF-8">
									<include name="**/*.java" />
									<include name="**/*.properties" />
								</fixcrlf>
								<echo>********** Done **************</echo>
							</tasks>
						</configuration>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<inherited>true</inherited>
				<configuration>
					<target>1.6</target>
					<source>1.6</source>
				</configuration>
			</plugin>
			<!--  Generate source jar -->
			<plugin>
				<artifactId>maven-source-plugin</artifactId>
				<executions>
					<execution>
						<id>attach-sources</id>
						<goals>
							<goal>jar</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.mortbay.jetty</groupId>
				<artifactId>maven-jetty-plugin</artifactId>
			</plugin>
